{
  "name": "lambda-static-server",
  "type": "module",
  "scripts": {
    "build": "npm run build-functions && npm run build-ui-app",
    "build-frontend-function": "cd functions/frontend && npm run build",
    "build-converter-function": "cd functions/converter && npm run build",
    "build-prepareConversion-function": "cd functions/prepareConversion && npm run build",
    "build-queryStatus-function": "cd functions/queryStatus && npm run build",
    "build-functions": "npm run build-frontend-function && npm run build-converter-function && npm run build-prepareConversion-function && npm run build-queryStatus-function",
    "build-ui-app": "cd ui && vite build",
    "dev:ui": "cd ui && vite",
    "prepack": "npm run build",
    "download-yt-dlp": "node  scripts/download-yt-dlp.js",
    "package": "npm run download-yt-dlp && npm run sam-package",
    "sam-package": "sam build",
    "ts-node": "ts-node"
  },
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.858.0",
    "@aws-sdk/client-dynamodb": "^3.864.0",
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/client-sfn": "^3.864.0",
    "@aws-sdk/s3-request-presigner": "^3.864.0",
    "aws-lambda": "^1.0.7"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.152",
    "@smithy/core": "^3.8.0",
    "@smithy/protocol-http": "^5.1.3",
    "@smithy/smithy-client": "^4.4.10",
    "@smithy/types": "^4.3.2",
    "@types/node": "^24.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "vite": "^7.0.6",
    "vite-plugin-singlefile": "^2.3.0"
  },
  "resolutions": {
    "@smithy/core": "latest",
    "@smithy/types": "latest"
  }
}
